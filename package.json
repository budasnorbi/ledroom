  "scripts": {
    "start": "nx serve",
    "build": "nx build",
    "test": "nx run-many --all --target=test --parallel",
    "lint": "nx workspace-lint && npx nx run-many --all --target=lint --parallel",
    "start:client": "env-cmd -f ./env/.env.development nx run client:serve",
    "start:api": "env-cmd -f ./env/.env.development nx run api:serve",
    "start:both": "env-cmd -f ./env/.env.development nx run-many --parallel --target=serve --projects=client,api",
    "prisma:generate": "npx prisma generate --schema=./apps/api/prisma/schema.prisma",
    "prisma:studio": "npx prisma studio --schema=./apps/api/prisma/schema.prisma",
    "prisma:migrate": "npx prisma migrate dev --schema=./apps/api/prisma/schema.prisma"
  },
